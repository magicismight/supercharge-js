{
  "name": "create-error-factory",
  "version": "0.0.0",
  "description": "A quick starter template for typescript project",
  "main": "dist/lib/create-error-factory.js",
  "module": "dist/esm/create-error-factory.js",
  "scripts": {
    "test": "jest --coverage",
    "lint": "eslint ./src --ext .ts",
    "build": "run-p test clean lint && run-p build:cjs build:esm",
    "build:cjs": "tsc --module commonjs --declaration --outDir ./dist/lib",
    "build:esm": "tsc --module esnext --declaration --outDir ./dist/esm",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist"
  },
  "license": "ISC",
  "author": {
    "name": "Horcrux Chen",
    "email": "master@horcrux.cn"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/magicismight/supercharge-js/packages/create-error-factor/"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
